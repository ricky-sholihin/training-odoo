<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_qty_wizard_form" model="ir.ui.view">
        <field name="name">qty.wizard.form</field>
        <field name="model">qty.wizard</field>
        <field name="arch" type="xml">
            <form string="Quantity Wizard">
                <group>
                    <field name="quantity"/>
                    <field name="order_id" readonly="1"/>
                </group>
                <footer>
                    <button name="action_confirm" string="Confirm" type="object" class="btn-primary"/>
                    <button special="cancel" string="Cancel" />
                </footer>
            </form>
        </field>
    </record>

</odoo>
